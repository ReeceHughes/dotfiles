# Bash Aliases
# Reece Hughes

# File loaded by .bashrc on startup

# ls
alias ll='ls -alF'
alias la='ls -lah'
alias l='ls -CF'
alias cla='clear && ls -la'

# cd
alias ..='cd .. && la'

alias op='xdg-open'

# Programming
alias python='python3'

# fun
alias timeToGoHome='python3 ~/Documents/projects/timeToGoHome/ttgh.py'

